{
  if (factTableDesc.getName().equalsIgnoreCase(table) || factTableDesc.getIdentity().equalsIgnoreCase(table))   return factTableDesc;
  for (  TableDesc desc : lookupTableDescs) {
    if (desc.getName().equalsIgnoreCase(table) || desc.getIdentity().equalsIgnoreCase(table))     return desc;
  }
  throw new IllegalArgumentException("Table not found by " + table);
}
