{
  descName=descName.toUpperCase();
  List<IIInstance> list=listAllIIs();
  List<IIInstance> result=new ArrayList<IIInstance>();
  Iterator<IIInstance> it=list.iterator();
  while (it.hasNext()) {
    IIInstance ci=it.next();
    if (descName.equals(ci.getDescName())) {
      result.add(ci);
    }
  }
  return result;
}
