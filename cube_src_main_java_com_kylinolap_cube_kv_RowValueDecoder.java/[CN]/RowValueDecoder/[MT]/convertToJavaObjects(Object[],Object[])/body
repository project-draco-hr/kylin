{
  for (int i=0; i < mapredObjs.length; i++) {
    Object o=mapredObjs[i];
    if (o instanceof LongWritable)     o=((LongWritable)o).get();
 else     if (o instanceof IntWritable)     o=((IntWritable)o).get();
 else     if (o instanceof DoubleWritable)     o=((DoubleWritable)o).get();
 else     if (o instanceof FloatWritable)     o=((FloatWritable)o).get();
    results[i]=o;
  }
}
