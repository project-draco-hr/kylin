{
  if (colMask == 0)   return 0;
  int count=1;
  for (  HierarchyMask hierMask : cube.getRowkey().getHierarchyMasks()) {
    long hierBits=colMask & hierMask.fullMask;
    if (hierBits != 0) {
      count*=Long.bitCount(hierBits) + 1;
      colMask&=~hierBits;
    }
  }
  count*=Math.pow(2,Long.bitCount(colMask));
  return count;
}
