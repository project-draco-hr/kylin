{
  TimeSlice doneSlice=null;
  if (curTimePartition != rec.getTimePartition()) {
    doneSlice=doneSlice();
    curTimePartition=rec.getTimePartition();
    curSliceNo=0;
  }
 else   if (isFull()) {
    doneSlice=doneSlice();
  }
  nRecords++;
  for (int i=0; i < nColumns; i++) {
    containers[i].append(rec.getValueID(i));
  }
  return doneSlice;
}
