{
  LogicalProject project=call.rel(0);
  RelTraitSet traitSet=project.getTraitSet().replace(OLAPRel.CONVENTION);
  OLAPProjectRel olapProj=new OLAPProjectRel(project.getCluster(),traitSet,convert(project.getInput(),traitSet),project.getProjects(),project.getRowType(),project.getFlags());
  call.transformTo(olapProj);
}
