{
  IRealizationProvider p=providers.get(type);
  if (p == null)   throw new IllegalStateException("No provider for realization type " + type);
  try {
    return p.getRealization(name);
  }
 catch (  Exception ex) {
    logger.warn("Failed to load realization " + type + ":"+ name,ex);
    return null;
  }
}
