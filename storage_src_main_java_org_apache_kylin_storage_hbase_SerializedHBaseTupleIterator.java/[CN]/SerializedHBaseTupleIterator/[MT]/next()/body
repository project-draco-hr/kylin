{
  ITuple t=null;
  while (hasNext()) {
    if (segmentIterator.hasNext()) {
      t=segmentIterator.next();
      scanCount++;
      break;
    }
 else {
      try {
        segmentIterator.close();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
      segmentIterator=segmentIteratorIterator.next();
    }
  }
  return t;
}
