{
  Pair<Integer,String> r;
  if (remoteHost == null) {
    r=runNativeCommand(command,logAppender);
  }
 else {
    r=runRemoteCommand(command,logAppender);
  }
  if (r.getFirst() != 0) {
    throw new IOException("OS command error exit with " + r.getFirst() + " -- "+ command+ "\n"+ r.getSecond());
  }
  return r;
}
