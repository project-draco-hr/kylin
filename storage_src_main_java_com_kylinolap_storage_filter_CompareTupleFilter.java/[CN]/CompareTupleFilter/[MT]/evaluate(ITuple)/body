{
  String tupleValue=null;
  for (  TupleFilter filter : this.children) {
    if (!(filter instanceof ConstantTupleFilter)) {
      filter.evaluate(tuple);
      tupleValue=filter.getValues().iterator().next();
    }
  }
  if (nullString != null && nullString.equals(tupleValue)) {
    tupleValue=null;
  }
  if (tupleValue == null) {
    if (operator == FilterOperatorEnum.ISNULL)     return true;
 else     return false;
  }
  if (firstCondValue.equals(nullString))   return false;
  int comp=tupleValue.compareTo(firstCondValue);
  boolean result;
switch (operator) {
case EQ:
    result=comp == 0;
  break;
case NEQ:
result=comp != 0;
break;
case LT:
result=comp < 0;
break;
case LTE:
result=comp <= 0;
break;
case GT:
result=comp > 0;
break;
case GTE:
result=comp >= 0;
break;
case IN:
result=conditionValues.contains(tupleValue);
break;
default :
result=false;
}
return result;
}
