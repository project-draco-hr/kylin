{
  HiveTableReader reader=new HiveTableReader("default","test_kylin_fact");
  int rowNumber=0;
  while (reader.next()) {
    String[] row=reader.getRow();
    Assert.assertEquals(9,row.length);
    System.out.println(ArrayUtils.toString(row));
    rowNumber++;
  }
  reader.close();
  Assert.assertEquals(10000,rowNumber);
}
