{
  while (true) {
    AbstractExecutable job=jobService.getJob(jobId);
    if (job.getStatus() == ExecutableState.SUCCEED || job.getStatus() == ExecutableState.ERROR || job.getStatus() == ExecutableState.DISCARDED) {
      break;
    }
 else {
      try {
        sleep(5000);
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
}
