{
  staticCreateTestMetadata(MINICLUSTER_TEST_DATA);
  if (!clusterStarted) {
synchronized (HBaseMiniclusterMetadataTestCase.class) {
      if (!clusterStarted) {
        startupMiniClusterAndImportData();
        clusterStarted=true;
      }
    }
  }
 else {
    updateKylinConfigWithMinicluster();
  }
}
