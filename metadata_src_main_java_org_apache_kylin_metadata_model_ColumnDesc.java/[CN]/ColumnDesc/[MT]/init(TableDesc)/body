{
  this.table=table;
  if (name != null)   name=name.toUpperCase();
  if (id != null)   zeroBasedIndex=Integer.parseInt(id) - 1;
  DataType normalized=DataType.getInstance(datatype);
  if (normalized == null) {
    this.setDatatype(null);
  }
 else {
    this.setDatatype(normalized.toString());
  }
}
