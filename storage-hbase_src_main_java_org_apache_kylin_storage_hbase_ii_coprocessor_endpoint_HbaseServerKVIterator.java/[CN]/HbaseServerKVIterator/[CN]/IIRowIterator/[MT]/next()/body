{
  if (results.size() < 1) {
    throw new NoSuchElementException();
  }
  for (  Cell c : results) {
    row.updateWith(c);
  }
  results.clear();
  try {
    if (hasMore) {
      hasMore=regionScanner.nextRaw(results);
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  return row;
}
