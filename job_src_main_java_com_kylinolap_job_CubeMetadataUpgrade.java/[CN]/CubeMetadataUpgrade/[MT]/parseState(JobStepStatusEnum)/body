{
switch (state) {
case NEW:
case PENDING:
case WAITING:
    return ExecutableState.READY;
case RUNNING:
  return ExecutableState.RUNNING;
case FINISHED:
return ExecutableState.SUCCEED;
case ERROR:
return ExecutableState.ERROR;
case DISCARDED:
return ExecutableState.DISCARDED;
default :
return ExecutableState.DISCARDED;
}
}
