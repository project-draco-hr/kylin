{
  this.bufferSize=0;
  int offset=0;
  int length=0;
  for (int i=0; i < byteLen; i++) {
    if (bytes[i] == delimiter) {
      SplittedBytes split=this.splitBuffers[this.bufferSize++];
      split.length=length;
      System.arraycopy(bytes,offset,split.value,0,length);
      offset=i + 1;
      length=0;
    }
 else {
      length++;
    }
  }
  SplittedBytes split=this.splitBuffers[this.bufferSize++];
  System.arraycopy(bytes,offset,split.value,0,length);
  split.length=length;
  return bufferSize;
}
