{
  lastKey=key;
  long length=0;
  for (  LongWritable v : values) {
    length+=v.get();
  }
  bytesRead+=length;
  if (bytesRead >= cut) {
    outputValue.set(bytesRead);
    context.write(key,outputValue);
    System.out.println(StringUtils.byteToHexString(key.getBytes()) + "\t" + outputValue.get());
    bytesRead=0;
  }
}
