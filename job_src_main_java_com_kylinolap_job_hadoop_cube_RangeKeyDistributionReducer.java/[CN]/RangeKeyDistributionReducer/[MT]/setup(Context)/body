{
  super.publishConfiguration(context.getConfiguration());
  CubeCapacity cubeCapacity=CubeCapacity.valueOf(context.getConfiguration().get(BatchConstants.CUBE_CAPACITY));
switch (cubeCapacity) {
case SMALL:
    cut=SMALL_CUT;
  break;
case MEDIUM:
cut=MEDIUM_CUT;
break;
case LARGE:
cut=LARGE_CUT;
break;
}
}
