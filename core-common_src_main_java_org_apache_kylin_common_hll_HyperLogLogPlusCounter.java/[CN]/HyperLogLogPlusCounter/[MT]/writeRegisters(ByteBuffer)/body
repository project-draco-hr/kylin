{
  final int indexLen=getRegisterIndexSize();
  int size=size();
  byte scheme;
  if (5 + (indexLen + 1) * size < m)   scheme=0;
 else   scheme=1;
  out.put(scheme);
  if (scheme == 0) {
    BytesUtil.writeVInt(size,out);
    for (int i=0; i < m; i++) {
      if (registers[i] > 0) {
        writeUnsigned(i,indexLen,out);
        out.put(registers[i]);
      }
    }
  }
 else   if (scheme == 1) {
    out.put(registers);
  }
 else   throw new IllegalStateException();
}
