{
  if (dimensions.intersects(info.colIsMetrics) || metrics.intersects(info.colIsDimension))   throw new IllegalStateException();
  this.info=info;
  this.dimensions=dimensions;
  this.metrics=metrics;
  this.filter=filter;
  this.rawScanner=new GTRawScanner(info,store,pkStart,pkEndExclusive,dimensions,metrics,filter);
}
