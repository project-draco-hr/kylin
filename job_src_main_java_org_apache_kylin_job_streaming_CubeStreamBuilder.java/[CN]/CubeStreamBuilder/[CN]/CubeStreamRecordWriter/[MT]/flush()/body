{
  try {
    if (!puts.isEmpty()) {
      long t=System.currentTimeMillis();
      if (hTable != null) {
        hTable.put(puts);
        hTable.flushCommits();
      }
      logger.info("commit total " + puts.size() + " puts, totally cost:"+ (System.currentTimeMillis() - t)+ "ms");
      puts.clear();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
