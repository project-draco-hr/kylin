{
  return new DimensionEncodingFactory(){
    @Override public String getSupportedEncodingName(){
      return ENCODING_NAME;
    }
    @Override public DimensionEncoding createDimensionEncoding(    String encodingName,    String[] args){
      return new FixedLenDimEnc(Integer.parseInt(args[0]));
    }
  }
;
}
