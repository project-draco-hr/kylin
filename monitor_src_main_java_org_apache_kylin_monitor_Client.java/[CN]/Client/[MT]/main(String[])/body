{
  logger.info("monitor client start parsing...");
  QueryParser queryParser=new QueryParser();
  HiveJdbcClient jdbcClient=new HiveJdbcClient();
  try {
    MonitorMetaManager.init();
    queryParser.start();
    jdbcClient.start();
  }
 catch (  Exception e) {
    logger.info("Exception",e);
  }
}
