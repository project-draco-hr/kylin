{
  counter++;
  if (counter % BatchConstants.COUNTER_MAX == 0) {
    logger.info("Handled " + counter + " records!");
  }
  try {
    bytesSplitter.split(value.getBytes(),value.getLength(),byteRowDelimiter);
    outputKV(context);
  }
 catch (  Exception ex) {
    handleErrorRecord(bytesSplitter,ex);
  }
}
