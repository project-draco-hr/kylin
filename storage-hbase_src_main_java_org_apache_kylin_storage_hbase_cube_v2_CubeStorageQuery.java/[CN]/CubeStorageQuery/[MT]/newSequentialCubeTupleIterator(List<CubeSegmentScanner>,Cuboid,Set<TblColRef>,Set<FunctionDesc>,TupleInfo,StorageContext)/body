{
  TblColRef topNCol=null;
  for (  FunctionDesc func : metrics) {
    if (func.isTopN()) {
      topNCol=func.getTopNLiteralColumn();
      break;
    }
  }
  if (topNCol != null)   return new SequentialCubeTopNTupleIterator(scanners,cuboid,dimensionsD,topNCol,metrics,returnTupleInfo,context);
 else   return new SequentialCubeTupleIterator(scanners,cuboid,dimensionsD,metrics,returnTupleInfo,context);
}
