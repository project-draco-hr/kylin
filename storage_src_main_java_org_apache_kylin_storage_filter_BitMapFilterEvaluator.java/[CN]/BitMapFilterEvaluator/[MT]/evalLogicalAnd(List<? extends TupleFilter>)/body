{
  int i=0;
  RoaringBitmap answer=null;
  for (; (i < children.size()) && (answer == null); ++i) {
    RoaringBitmap t=evaluate(children.get(i));
    if (t == null)     continue;
    answer=t;
  }
  for (; i < children.size(); ++i) {
    RoaringBitmap t=evaluate(children.get(i));
    if (t == null)     continue;
    answer.and(t);
  }
  if (answer == null)   answer=new RoaringBitmap();
  return answer;
}
