{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  TableRecordInfo other=(TableRecordInfo)obj;
  if (seg == null) {
    if (other.seg != null)     return false;
  }
 else   if (!seg.equals(other.seg))   return false;
  return true;
}
