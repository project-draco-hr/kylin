{
  try {
    beforeClass();
    BuildCubeWithStream buildCubeWithStream=new BuildCubeWithStream();
    buildCubeWithStream.before();
    buildCubeWithStream.build();
    logger.info("Build is done");
    afterClass();
    logger.info("Going to exit");
    System.exit(0);
  }
 catch (  Exception e) {
    logger.error("error",e);
    System.exit(1);
  }
}
