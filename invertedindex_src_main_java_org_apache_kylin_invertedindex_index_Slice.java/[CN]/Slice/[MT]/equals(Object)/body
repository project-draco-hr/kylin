{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Slice other=(Slice)obj;
  if (shard != other.shard) {
    return false;
  }
  if (timestamp != other.timestamp) {
    return false;
  }
  return Objects.equal(info,other.info);
}
