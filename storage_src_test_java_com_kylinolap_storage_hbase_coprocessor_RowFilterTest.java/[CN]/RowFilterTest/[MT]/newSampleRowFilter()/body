{
  ColumnFilter[][] filter=new ColumnFilter[4][];
  filter[0]=new ColumnFilter[]{newFilterA(new byte[]{0x01},new byte[]{0x01})};
  filter[1]=new ColumnFilter[]{newFilterB(new byte[]{0x02,0x02},new byte[]{0x02,0x03}),newFilterC(new byte[]{0x03,0x03,0x03},new byte[]{0x03,0x03,0x04})};
  filter[2]=null;
  filter[3]=new ColumnFilter[0];
  return new RowFilter(filter);
}
