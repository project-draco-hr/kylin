{
  if (cacheManager.getCache(storageClassName) == null) {
    logger.info("Cache for {} initting:",storageClassName);
    Cache storageCache=new Cache(new CacheConfiguration(storageClassName,0).memoryStoreEvictionPolicy(MemoryStoreEvictionPolicy.LRU).eternal(false).timeToIdleSeconds(86400).diskExpiryThreadIntervalSeconds(0).maxBytesLocalHeap(256,MemoryUnit.MEGABYTES).persistence(new PersistenceConfiguration().strategy(PersistenceConfiguration.Strategy.NONE)));
    cacheManager.addCache(storageCache);
  }
}
