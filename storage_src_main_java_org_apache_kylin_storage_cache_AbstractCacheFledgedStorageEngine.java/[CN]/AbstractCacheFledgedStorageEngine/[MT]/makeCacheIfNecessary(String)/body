{
  if (cacheManager.getCache(realizationUUID) == null) {
    logger.info("Cache for {} initting...",realizationUUID);
    Cache storageCache=new Cache(new CacheConfiguration(realizationUUID,0).memoryStoreEvictionPolicy(MemoryStoreEvictionPolicy.LRU).eternal(false).timeToIdleSeconds(86400).diskExpiryThreadIntervalSeconds(0).persistence(new PersistenceConfiguration().strategy(PersistenceConfiguration.Strategy.NONE)));
    cacheManager.addCache(storageCache);
  }
}
