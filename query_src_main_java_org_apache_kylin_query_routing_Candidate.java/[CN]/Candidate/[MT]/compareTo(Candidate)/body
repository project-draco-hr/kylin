{
  int comp=this.priority - o.priority;
  if (comp != 0) {
    return comp;
  }
  comp=this.capability.cost - o.capability.cost;
  if (comp != 0) {
    return comp;
  }
  if (this.realization instanceof HybridInstance)   return -1;
 else   if (o.realization instanceof HybridInstance)   return 1;
 else   return 0;
}
