{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  TblColRef other=(TblColRef)obj;
  if (!StringUtils.equals(TableDesc.getTableIdentity(column.getTable()),TableDesc.getTableIdentity(other.column.getTable())))   return false;
  if (!StringUtils.equals(column.getName(),other.column.getName()))   return false;
  return true;
}
