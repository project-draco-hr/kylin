{
  if (colValues.size() > 0) {
    outputDistinctValues(columnList.get((int)currentColIndex),colValues,context);
    colValues.clear();
  }
  if (collectStatistics) {
    long grandTotal=0;
    for (    HyperLogLogPlusCounter hll : cuboidHLLMap.values()) {
      grandTotal+=hll.getCountEstimate();
    }
    double mapperOverlapRatio=(double)totalRowsBeforeMerge / grandTotal;
    writeMapperAndCuboidStatistics(context);
    CuboidStatsUtil.writeCuboidStatistics(context.getConfiguration(),new Path(statisticsOutput),cuboidHLLMap,samplingPercentage,mapperOverlapRatio);
  }
}
