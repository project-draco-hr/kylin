{
  results.clear();
  try {
    hasMore=regionScanner.next(results);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  if (results.size() < 1) {
    throw new IllegalStateException("Hbase row contains less than 1 cell");
  }
  for (  Cell c : results) {
    row.updateWith(c);
  }
  return row;
}
