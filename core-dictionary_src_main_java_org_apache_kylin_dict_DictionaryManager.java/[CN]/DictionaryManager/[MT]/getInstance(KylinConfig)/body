{
  DictionaryManager r=CACHE.get(config);
  if (r == null) {
synchronized (DictionaryManager.class) {
      r=CACHE.get(config);
      if (r == null) {
        r=new DictionaryManager(config);
        CACHE.put(config,r);
        if (CACHE.size() > 1) {
          logger.warn("More than one singleton exist");
        }
      }
    }
  }
  return r;
}
