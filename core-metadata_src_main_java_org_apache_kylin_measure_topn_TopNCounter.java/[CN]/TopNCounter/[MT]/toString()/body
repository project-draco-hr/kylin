{
  StringBuilder sb=new StringBuilder();
  sb.append('[');
  for (ListNode2<Counter<T>> bNode=counterList.head(); bNode != null; bNode=bNode.getPrev()) {
    Counter<T> b=bNode.getValue();
    sb.append(b.item);
    sb.append(':');
    sb.append(b.count);
  }
  sb.append(']');
  return sb.toString();
}
