{
  double[] counters=new double[size()];
  int index=0;
  for (ListNode2<Bucket> bNode=bucketList.head(); bNode != null; bNode=bNode.getPrev()) {
    Bucket b=bNode.getValue();
    for (    Counter<T> c : b.counterList) {
      counters[index]=c.count;
      index++;
    }
  }
  return counters;
}
