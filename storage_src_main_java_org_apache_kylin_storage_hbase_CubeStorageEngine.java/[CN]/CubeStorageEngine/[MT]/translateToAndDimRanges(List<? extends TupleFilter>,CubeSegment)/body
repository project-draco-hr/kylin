{
  Map<TblColRef,ColumnValueRange> rangeMap=new HashMap<TblColRef,ColumnValueRange>();
  for (  TupleFilter filter : andFilters) {
    if ((filter instanceof CompareTupleFilter) == false) {
      continue;
    }
    CompareTupleFilter comp=(CompareTupleFilter)filter;
    if (comp.getColumn() == null) {
      continue;
    }
    @SuppressWarnings("unchecked") ColumnValueRange range=new ColumnValueRange(comp.getColumn(),(Collection<String>)comp.getValues(),comp.getOperator());
    andMerge(range,rangeMap);
  }
  return rangeMap.values();
}
