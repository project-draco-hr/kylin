{
  ServletServer server=null;
  try {
    server=serverStart(args);
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    log.error(ex.getMessage(),ex);
  }
  long start=System.nanoTime();
  try {
    if (server != null) {
      waitForKillSignal();
    }
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    log.error(ex.getMessage(),ex);
  }
  log.info("Killing webhead, uptime: %d ms",(System.nanoTime() - start) / MS_PER_NANO);
  try {
    if (server != null) {
      serverStop(server);
    }
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    log.error(ex.getMessage(),ex);
  }
}
