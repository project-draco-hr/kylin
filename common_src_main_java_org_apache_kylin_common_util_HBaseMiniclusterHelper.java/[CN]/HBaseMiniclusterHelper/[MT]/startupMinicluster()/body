{
  HBaseMetadataTestCase.staticCreateTestMetadata(HBaseMetadataTestCase.MINICLUSTER_TEST_DATA);
  if (!clusterStarted) {
synchronized (HBaseMiniclusterHelper.class) {
      if (!clusterStarted) {
        startupMiniClusterAndImportData();
        clusterStarted=true;
      }
    }
  }
 else {
    updateKylinConfigWithMinicluster();
  }
}
