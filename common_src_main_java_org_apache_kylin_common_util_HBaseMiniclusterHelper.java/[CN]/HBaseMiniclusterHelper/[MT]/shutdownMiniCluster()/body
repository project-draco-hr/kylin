{
  System.out.println("Going to shutdown mini cluster.");
  if (!StringUtils.isEmpty(hbaseconnectionUrl)) {
    HBaseConnection.removeConfig(hbaseconnectionUrl);
  }
  try {
    UTIL.shutdownMiniMapReduceCluster();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    UTIL.shutdownMiniCluster();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
