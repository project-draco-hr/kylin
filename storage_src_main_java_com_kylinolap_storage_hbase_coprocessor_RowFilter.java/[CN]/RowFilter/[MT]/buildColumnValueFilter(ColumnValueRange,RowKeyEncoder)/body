{
  TblColRef col=range.getColumn();
  int colOffset=encoder.getColumnOffset(col);
  int colLength=encoder.getColumnLength(col);
  byte[] valueStart=encodeColumnValue(range.getBeginValue(),col,colOffset,colLength,RowConstants.ROWKEY_LOWER_BYTE,encoder);
  byte[] valueEnd=encodeColumnValue(range.getEndValue(),col,colOffset,colLength,RowConstants.ROWKEY_UPPER_BYTE,encoder);
  return new ColumnFilter(colOffset,colLength,valueStart,valueEnd);
}
