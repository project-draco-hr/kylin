{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  ColumnFilter other=(ColumnFilter)obj;
  if (columnLength != other.columnLength)   return false;
  if (columnOffset != other.columnOffset)   return false;
  if (!Arrays.equals(valueEnd,other.valueEnd))   return false;
  if (!Arrays.equals(valueStart,other.valueStart))   return false;
  return true;
}
