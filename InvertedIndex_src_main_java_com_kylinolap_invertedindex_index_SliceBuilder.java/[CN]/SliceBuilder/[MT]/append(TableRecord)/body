{
  if (rec.getShard() != shard)   throw new IllegalStateException();
  Slice doneSlice=null;
  if (isFull()) {
    doneSlice=doneSlice();
  }
  if (nRecords == 0) {
    sliceTimestamp=increaseSliceTimestamp(rec.getTimestamp());
  }
  nRecords++;
  for (int i=0; i < nColumns; i++) {
    rec.getValueBytes(i,temp);
    containers[i].append(temp);
  }
  return doneSlice;
}
