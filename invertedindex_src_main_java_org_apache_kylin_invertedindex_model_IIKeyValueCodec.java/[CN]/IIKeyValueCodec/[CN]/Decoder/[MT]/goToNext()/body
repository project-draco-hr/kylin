{
  if (slice != null) {
    return;
  }
  while (slice == null && iterator.hasNext()) {
    IIRow kv=iterator.next();
    ImmutableBytesWritable k=kv.getKey();
    ImmutableBytesWritable v=kv.getValue();
    decodeKey(k);
    if (curShard != lastShard || curSliceTimestamp != lastSliceTimestamp) {
      makeNext();
    }
    consumeCurrent(v);
  }
  if (slice == null) {
    makeNext();
  }
}
