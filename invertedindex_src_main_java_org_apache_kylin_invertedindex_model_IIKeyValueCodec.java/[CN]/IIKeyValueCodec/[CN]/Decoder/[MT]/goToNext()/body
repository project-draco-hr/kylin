{
  if (next != null) {
    return;
  }
  while (next == null && iterator.hasNext()) {
    KeyValuePair kv=iterator.next();
    ImmutableBytesWritable k=kv.getKey();
    ImmutableBytesWritable v=kv.getValue();
    decodeKey(k);
    if (curShard != lastShard || curSliceTimestamp != lastSliceTimestamp) {
      makeNext();
    }
    consumeCurrent(v);
  }
  if (next == null) {
    makeNext();
  }
}
