{
  try {
    node.accept(new RexVisitorImpl<Void>(true){
      @Override public Void visitCall(      RexCall call){
        if (call.getOperator() == RexBuilder.GET_OPERATOR) {
          throw Util.FoundOne.NULL;
        }
        return super.visitCall(call);
      }
    }
);
    return false;
  }
 catch (  Util.FoundOne e) {
    return true;
  }
}
