{
  Configuration conf=new Configuration();
  conf.setMaxBytesLocalHeap("128M");
  CACHE_MANAGER=CacheManager.create(conf);
  Cache storageCache=new Cache(new CacheConfiguration(storageCacheTemplate,0).memoryStoreEvictionPolicy(MemoryStoreEvictionPolicy.LRU).eternal(false).timeToIdleSeconds(86400).diskExpiryThreadIntervalSeconds(0).persistence(new PersistenceConfiguration().strategy(PersistenceConfiguration.Strategy.NONE)));
  CACHE_MANAGER.addCacheIfAbsent(storageCache);
}
