{
  int pos=buf.position();
  for (int i=0; i < info.colAll.trueBitCount(); i++) {
    int c=info.colAll.trueBitAt(i);
    byte exist=buf.get();
    pos++;
    if (exist == 1) {
      int len=info.codeSystem.codeLength(c,buf);
      cols[c].set(buf.array(),buf.arrayOffset() + pos,len);
      pos+=len;
      buf.position(pos);
    }
  }
}
