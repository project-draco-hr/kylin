{
  List<String> result=Lists.newArrayList();
  for (  String t : list) {
    t=t.trim();
    if (t.endsWith(","))     t=t.substring(0,t.length() - 1);
    if (t.equals("all")) {
      result.addAll(tableNames);
      break;
    }
    if (tableNames.contains(t)) {
      result.add(t);
    }
  }
  return result;
}
