{
  StringBuffer sb=new StringBuffer();
  sb.append("[");
  for (  Candidate candidate : candidates) {
    IRealization r=candidate.realization;
    sb.append(r.getName());
    sb.append(",");
  }
  if (sb.charAt(sb.length() - 1) != '[')   sb.deleteCharAt(sb.length() - 1);
  sb.append("]");
  return sb.toString();
}
